{
  "name": "vue-slider-with-indicator",
  "version": "1.0.11",
  "main": "dist/vue-slider-with-indicator.umd.js",
  "module": "dist/vue-slider-with-indicator.esm.js",
  "unpkg": "dist/vue-slider-with-indicator.min.js",
  "browser": {
    "./sfc": "src/vue-slider-with-indicator.vue"
  },
  "description": "A slider with an indicator for an additional value (e.g. for synths)",
  "author": "Mathias B",
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-slider-with-indicator.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-slider-with-indicator.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-slider-with-indicator.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/viruslight/vue-slider-with-indicator"
  },
  "dependencies": {
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "rollup": "^3.29.5",
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue-template-compiler": "^2.5.2"
  },
  "license": "ISC"
}
